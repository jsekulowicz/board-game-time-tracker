{
  "paths": {
    "/games": {
      "get": {
        "summary": "List all games",
        "operationId": "listGames",
        "responses": {
          "200": {
            "description": "List of games",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "../components/schemas/games.json#/schemas/Game"
                  }
                }
              }
            }
          },
          "500": {
            "$ref": "../components/responses.json#/responses/Error500"
          }
        }
      }
    }
  }
}

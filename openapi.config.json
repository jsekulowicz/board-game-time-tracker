{
  "$schema": "https://unpkg.com/@hey-api/openapi-ts/config-schema.json",
  "input": "openapi/openapi.json",
  "output": {
    "path": "src/api/generated",
    "format": "prettier"
  },
  "plugins": {
    "typescript": {
      "enumStyle": "asConst",
      "inferTypes": true,
      "name": "ApiTypes"
    },
    "client": {
      "name": "ApiClient",
      "httpClient": "fetch"
    }
  },
  "resolver": {
    "baseDir": "openapi"
  }
}
